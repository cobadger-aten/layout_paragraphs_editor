{% if is_empty %}
<div class="lpe-enable__empty-message__wrapper">
  <div class="lpe-enable__empty-message">
    <p>
      You haven't created any {{ field_label }} yet.
      <a class="lpe-enable-button" href="{{url}}">{% trans %}Start creating {{ field_label }}.{% endtrans %}</a>
    </p>
  </div>
</div>
{% else %}
<div class="lpe-enable__wrapper">
  <div class="lpe-enable">
    <div class="lpe-enable__button"><a class="lpe-enable-button" href="{{url}}"><span>{{title}}</span></a></div>
  </div>
</div>
{% endif %}